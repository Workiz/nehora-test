{
  "name": "microservice-for-infra-tests",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint src/**/*.js",
    "lintFix": "eslint src/**/*.js --fix",
    "startDev": "NODE_ENV=development npm run start",
    "test": "npm run testIntegration && npm run testBlackbox",
    "testIntegration": "NODE_ENV=test mocha --file tests/integration/setup.js 'tests/integration/**/*.spec.js' --exit",
    "testBlackbox": "NODE_ENV=test mocha --file tests/blackbox/setup.js 'tests/blackbox/**/*.spec.js' --exit",
    "startTestsContainers": "docker-compose -f tests/docker-compose.yml -p node-service up -d",
    "stopTestsContainers": "docker-compose -f tests/docker-compose.yml -p node-service down",
    "audit": "npx audit-ci --moderate"
  },
  "author": "",
  "license": "ISC",
  "repository": "https://github.com/Workiz/architecture",
  "homepage": "https://github.com/Workiz/architecture/templates/generator-micro-service",
  "dependencies": {
    "@google-cloud/secret-manager": "^3.10.1",
    "@octokit/core": "^4.2.0",
    "@workiz/generator-micro-service": "^1.2.6",
    "@workiz/generator-micro-service-ts": "^1.2.8",
    "@workiz/message-bus": "^2.1.3",
    "@workiz/node-logger": "1.0.5",
    "audit-ci": "^5.1.2",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "follow-redirects": "^1.14.7",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.13",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "prom-client": "^14.0.1",
    "redis": "^4.0.0",
    "sequelize": "^6.12.0-beta.1",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.12",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "mocha": "^9.1.3"
  }
}
